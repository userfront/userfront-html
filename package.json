{
  "name": "@userfront/html",
  "version": "1.0.18-beta.1",
  "description": "Userfront HTML binding",
  "source": "src/index.js",
  "main": "build/userfront-html.js",
  "module": "build/userfront-html.module.js",
  "unpkg": "build/userfront-html.umd.js",
  "scripts": {
    "test": "jest --watch",
    "build": "npm version patch && microbundle",
    "pub": "npm publish",
    "build:beta": "npm version prerelease --preid=beta && microbundle",
    "pub:beta": "npm publish --tag beta",
    "reinstall": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userfront/userfront-html.git"
  },
  "keywords": [
    "userfront"
  ],
  "author": "Userfront",
  "bugs": {
    "url": "https://github.com/userfront/userfront-html/issues"
  },
  "homepage": "https://github.com/userfront/userfront-html#readme",
  "dependencies": {
    "@anymod/html": "^3.0.22",
    "@userfront/core": "^0.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "es6-promise": "^4.2.8",
    "jest": "^26.6.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "microbundle": "^0.12.4",
    "nock": "^13.0.4"
  }
}
